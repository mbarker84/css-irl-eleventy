{
  "name": "css-irl-eleventy",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "run-s start:remove build:images build:assets start:watch",
    "start:remove": "node onStart.mjs",
    "start:watch": "run-p watch:eleventy watch:parcel start:browsersync",
    "watch:eleventy": "npx @11ty/eleventy --watch",
    "watch:parcel": "run-p watch:css watch:js",
    "watch:css": "parcel watch src/css/styles.scss",
    "watch:js": "parcel watch src/js/index.js src/js/service-worker.js",
    "watch:images": "parcel watch src/images/*",
    "build": "run-s clean prod:parcel build:assets hash prod:eleventy",
    "svg": "svg-symbol-sprite -i src/icons -o src/_includes/partials/sprite.svg -p ''",
    "prod:eleventy": "npx @11ty/eleventy",
    "prod:parcel": "run-p build:css build:js build:images",
    "build:css": "parcel build src/css/styles.scss",
    "build:js": "parcel build src/js/index.js src/js/service-worker.js",
    "build:images": "parcel build src/images/*",
    "build:assets": "node copyAssets.mjs",
    "start:browsersync": "browser-sync start --config bs-config.js",
    "clean": "rm -rf dist",
    "hash": "node onBuild.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@parcel/transformer-sass": "^2.9.3",
    "@parcel/transformer-webmanifest": "^2.9.3",
    "browser-sync": "^2.29.3",
    "eleventy-plugin-webmentions": "^2.1.0",
    "eleventy-plugin-youtube-embed": "^1.9.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "md5": "^2.3.0",
    "netlify-cli": "^12.14.0",
    "npm-run-all": "^4.1.5",
    "os-browserify": "^0.3.0",
    "parcel": "^2.9.3",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "sass": "^1.69.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "svg-symbol-sprite": "^1.4.1"
  },
  "packageManager": "npm@8.1.2",
  "dependencies": {
    "body-scroll-lock": "^4.0.0-beta.0",
    "dotenv": "^16.1.4",
    "focus-trap": "^7.4.3"
  }
}
